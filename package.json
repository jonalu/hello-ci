{
  "name": "hello-ci",
  "version": "1.0.0",
  "description": "CI hello world",
  "main": "server.js",
  "watch": {
    "build:scss": "app/scss/main.scss",
    "build:js": "app/**/*.js"
  },
  "scripts": {
    "start": "node run.js",
    "dev": "nodemon run.js",
    "test": "_mocha --colors --recursive -R list",
    "watch": "npm-watch",
    "test:watch": "npm test -- --watch",
    "build:scss": "node-sass app/scss/main.scss | postcss --use autoprefixer --autoprefixer.browsers \"> 5%\" -o public/style.css",
    "build:js": "webpack --progress --colors --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonalu/hello-ci.git"
  },
  "keywords": [
    "nodejs",
    "docker",
    "ci"
  ],
  "author": "Jonas Lundeland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonalu/hello-ci/issues"
  },
  "homepage": "https://github.com/jonalu/hello-ci#readme",
  "dependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.4",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "classnames": "^2.2.3",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "moment": "^2.12.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "socket.io": "^1.4.5",
    "superagent": "^1.8.3",
    "superagent-as-promised": "^4.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.5",
    "babel-loader": "^6.2.4",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "npm-watch": "^0.1.1",
    "postcss-cli": "^2.5.1",
    "webpack": "^1.12.14"
  }
}
